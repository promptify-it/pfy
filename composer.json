{
    "name": "danieletulone/pfy",
    "type": "project",
    "require-dev": {
        "configured/haulable": "^0.4.0",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^2.9",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^9.0.0||^8.22.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "spatie/laravel-ignition": "^2.4",
        "symplify/monorepo-builder": "^11.2"
    },
    "autoload": {
        "psr-4": {
            "Database\\Factories\\": [
                "apps/cli/database/factories/",
                "apps/web/database/factories/"
            ],
            "Database\\Seeders\\": [
                "apps/cli/database/seeders/",
                "apps/web/database/seeders/"
            ],
            "Pfy\\Cli\\": "apps/cli/app/",
            "Pfy\\Core\\": "packages/core/src/",
            "Pfy\\Web\\": "apps/web/app/"
        }
    },
    "authors": [
        {
            "name": "Daniele Tulone",
            "email": "danieletulone.work@gmail.com"
        },
        {
            "name": "Nuno Maduro",
            "email": "enunomaduro@gmail.com"
        },
        {
            "name": "danieletulone",
            "email": "danieletulone.work@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/contracts": "^10.0||^11.0",
        "illuminate/http": "^11.5",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel-zero/framework": "^11.0.0",
        "laravel/framework": "^11.0",
        "laravel/jetstream": "^5.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "php": "^8.2",
        "promptify-it/promptify-it": "dev-main",
        "spatie/laravel-data": "^4.5",
        "spatie/laravel-package-tools": "^1.16",
        "tightenco/ziggy": "^2.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload-dev": {
        "psr-4": {
            "Pfy\\Core\\Tests\\": "packages/core/tests/",
            "Symplify\\Tests\\": "tests",
            "Tests\\": [
                "apps/cli/tests/",
                "apps/web/tests/"
            ],
            "Workbench\\App\\": "packages/core/workbench/app/"
        }
    },
    "repositories": [
        {
            "type": "github",
            "url": "https://github.com/promptify-it/core"
        }
    ],
    "extra": {
        "laravel": {
            "aliases": {
                "PromptifyIt": "PromptifyIt\\PromptifyIt\\Facades\\PromptifyIt"
            },
            "providers": [
                "PromptifyIt\\PromptifyIt\\PromptifyItServiceProvider"
            ]
        }
    },
    "replace": {
        "promptify-it/cli": "self.version",
        "promptify-it/promptify-it": "self.version",
        "promptify-it/web": "self.version"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}